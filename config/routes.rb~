MelissaBeasonSmith::Application.routes.draw do
  root :to => 'visitor#new'

	resources :visitor

	match '/trauma', 				:to => 'visitor#trauma'
	match '/anxiety', 			:to => 'visitor#anxiety'
	match '/borderline', 		:to => 'visitor#borderline'
	match '/grief', 				:to => 'visitor#grief'
	match '/life', 					:to => 'visitor#life'
	match '/relationship',  :to => 'visitor#relationship'
	match '/home',					:to => 'visitor#new'
	match '/about_me', 			:to => 'visitor#aboutMe'
	match '/why_therapy', 	:to => 'visitor#whyTherapy'
	match '/contact', 			:to => 'visitor#contact'
	match '/specialization', :to => 'visitor#specialization'
	match '/navbar',				 :to => 'visitor#navbar'

end
